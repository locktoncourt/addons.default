<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.tv.inter" name="Télé International" version="1.0.1.1" provider-name="Angelscry">
    <requires>
        <import addon="xbmc.python" version="2.0"/>
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <platform>all</platform>
        <disclaimer>For XBMC Eden/Frodo</disclaimer>
        <summary lang="en">Watch live International channels.</summary>
        <description lang="en">This add-on proposes to view most of the international channels from live video streams available on the internet. The proposed channels are classified by content with a detailed description.</description>
    </extension>
</addon>
